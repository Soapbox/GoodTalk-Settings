{
    "name": "soapbox/goodtalk-settings",
    "description": "A package to handle settings.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hayes",
            "email": "justin@soapboxhq.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/soapbox/eloquent-model-validation"
        }
    ],
    "require": {
        "illuminate/support": "^9.0.0",
        "illuminate/database": "^9.0.0",
        "symfony/cache": "^6.0.19"
    },
    "require-dev": {
        "orchestra/testbench": "^7.41.3",
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.23.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\Settings\\": "src",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    }
}
