{
    "name": "soapbox/goodtalk-settings",
    "description": "A package to handle settings.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hayes",
            "email": "justin@soapboxhq.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/soapbox/eloquent-model-validation"
        }
    ],
    "require": {
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "symfony/cache": "^5.4.39"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "~8.0",
        "fakerphp/faker": "^1.23.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    }
}
